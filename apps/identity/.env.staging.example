# Staging Environment Configuration (EXAMPLE)
# Copy this to .env.staging and update with real values
NODE_ENV=staging
PORT=3000

# Database
DATABASE_HOST=postgres
DATABASE_PORT=5432
DATABASE_USER=agentkern
DATABASE_PASSWORD=your_staging_password_here
DATABASE_NAME=agentkern_identity
DATABASE_SYNC=true
DATABASE_SSL=false

# PostgreSQL Container
POSTGRES_USER=agentkern
POSTGRES_PASSWORD=your_staging_password_here
POSTGRES_DB=agentkern_identity

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# WebAuthn
WEBAUTHN_RP_NAME=AgentKern Identity Staging
WEBAUTHN_RP_ID=localhost
WEBAUTHN_ORIGIN=http://localhost:3000
